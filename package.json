{
  "name": "react-breadcrumbs",
  "version": "2.1.2",
  "description": "Automatic breadcrumbs for react-router",
  "author": "Sven Anders Robbestad <anders@robbestad.com>",
  "homepage": "https://github.com/svenanders/react-breadcrumbs",
  "license": "ISC",
  "main": "dist/react-breadcrumbs.min.js",
  "scripts": {
    "build": "webpack",
    "release": "standard-version",
    "test": "mocha --require babel-register test/test/**/*",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,jsx}": [
      "prettier --semi false --single-quote false --write",
      "git add"
    ]
  },
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/svenanders/react-breadcrumbs.git"
  },
  "bugs": {
    "url": "https://github.com/svenanders/react-breadcrumbs/issues"
  },
  "keywords": [
    "react-component",
    "breadcrumbs",
    "react",
    "trail"
  ],
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": ">=15.0.0 || >=16.0.0",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "jsdom": "^11.3.0",
    "lint-staged": "^4.3.0",
    "mocha": "^4.0.1",
    "prettier": "^1.8.1",
    "prop-types": "^15.5.8",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^16.0.0",
    "standard-version": "^4.2.0",
    "webpack": "^3.5.6"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "redux": "^3.6.0",
    "uuid": "^3.0.1"
  }
}
